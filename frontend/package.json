{
  "name": "speedhockey.online",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "protoc '--plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd' --ts_proto_out=. ./speedhockey-backend/src/interface.proto",
    "build": "cross-env NODE_ENV=production parcel build src/index.html",
    "start": "cross-env NODE_ENV=development parcel src/index.html",
    "serve": "tsc && node --env-file=.env.production dist/server.js",
    "serve-dev": "tsc && node --env-file=.env.development dist/server.js"
  },
  "license": "MIT",
  "dependencies": {
    "matter-js": "^0.19.0",
    "p5": "^1.8.0",
    "parcel": "^2.10.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "ts-proto": "^1.169.0",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/matter-js": "^0.19.5",
    "@types/p5": "^1.7.4",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3"
  }
}
